{
  "name": "pokedex-ssr",
  "version": "1.0.0",
  "description": "Pokedex",
  "keywords": [
    "js",
    "json",
    "typescript",
    "react",
    "afterjs",
    "razzle"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viktoriabakun/pokedex-srr.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "razzle start",
    "export": "razzle export",
    "export:shell": "RAZZLE_SHELL_EXPORT=true razzle export",
    "build": "./build.sh",
    "build:dev": "NODE_ENV=development ./build.sh",
    "build:spa": "BUILD_TYPE=spa ./build.sh",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "prettier:format": "prettier --write 'src/**/*.{ts,tsx,*.ts,*tsx}'",
    "prettier:check": "prettier --check --debug-check 'src/**/*.{ts,tsx,*.ts,*tsx}'",
    "lint:check": "eslint --ext '.ts,.tsx' 'src/**/*.{ts,tsx,*.ts,*tsx}'",
    "lint:format": "eslint --fix --ext '.ts,.tsx' 'src/**/*.{ts,tsx,*.ts,*tsx}'",
    "ts:check": "tsc --project ./tsconfig.json --skipLibCheck --noemit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@jaredpalmer/after": "file:./node_modules/@lomray/after",
    "@loadable/component": "^5.15.2",
    "@lomray/after": "3.2.0",
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "express": "^4.17.3",
    "fast-deep-equal": "^3.1.3",
    "formik": "^2.2.9",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^20.3.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-backend": "^1.3.1",
    "i18next-http-middleware": "^3.1.4",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "razzle": "4.2.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.3",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-top-loading-bar": "^2.1.0",
    "universal-cookie-express": "^4.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@lomray/eslint-config-react": "^1.0.1",
    "@lomray/prettier-config": "^1.1.0",
    "@types/express": "^4.17.13",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jest": "^27.4.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.18.12",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.4",
    "@types/react-lazyload": "^3.1.0",
    "@types/react-router-dom": "^5.1.9",
    "@types/webpack-env": "^1.15.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-razzle": "4.2.16",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "^2.6.0",
    "razzle-dev-utils": "4.2.16",
    "razzle-plugin-scss": "4.2.16",
    "razzle-plugin-svg-react-component": "^2.0.0",
    "razzle-plugin-typescript": "4.2.16",
    "semantic-release": "^19.0.2",
    "serialize-javascript": "^6.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.5.1"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
